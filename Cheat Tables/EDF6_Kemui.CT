{$lua}
--Automatically sets up a playable monster,
--by Kemui52.
local addrPlayerParentSource = GetAddress(AddressList.getMemoryRecordByDescription("Player Parent (Source)").AddressString)
local valuPlayerParentSource = readQword(addrPlayerParentSource)
local addrLooseObject = GetAddress(AddressList.getMemoryRecordByDescription("Loose Object").AddressString)
local addrPlayerParentLoose = 0x340 --GetAddress(AddressList.getMemoryRecordByDescription("Player Parent (Loose)").AddressString)
local addrControlFlagsLoose = 0x354 --GetAddress(AddressList.getMemoryRecordByDescription("Control Flags (Loose)").AddressString)
local addrCameraFollowingObject = GetAddress(AddressList.getMemoryRecordByDescription("Following Object").AddressString)

if ReadQword(addrLooseObject) ~= nil and
   ReadQword(addrPlayerParentSource) ~= nil and
   valuPlayerParentSource ~= 0 then
--Give monster player controls.
 writeQword(addrLooseObject+addrPlayerParentLoose, valuPlayerParentSource)
 writeInteger(addrLooseObject+addrControlFlagsLoose, 1)
--Remove player's controls.
 writeQword(addrPlayerParentSource, 0)
--Attach camera to monster.
 writeQword(addrCameraFollowingObject, addrLooseObject)
else
 showMessage("Failed to start. Is the Loose Object section filled out?")
end
