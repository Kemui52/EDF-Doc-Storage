local CamYVal = readFloat(CamY);
local CamZVal = readFloat(CamZ);
local LookXVal = readFloat(LookX);
local LookYVal = readFloat(LookY);
local LookZVal = readFloat(LookZ);
if ActWind == getForegroundProcess() then

if isKeyPressed(VK_RSHIFT) then
 SpeedUpVal = 2
elseif isKeyPressed(VK_RCONTROL) then
 SpeedUpVal = 0.25
else
 SpeedUpVal = 1
end

if isKeyPressed(VK_O) then
writeFloat(CamZ, CamZVal + IncrementVal * SpeedUpVal);
end
if isKeyPressed(VK_P) then
writeFloat(CamZ, CamZVal - IncrementVal * SpeedUpVal);
end
if isKeyPressed(219) then
writeFloat(CamX, CamXVal + IncrementVal * SpeedUpVal);
end
if isKeyPressed(221) then
writeFloat(CamX, CamXVal - IncrementVal * SpeedUpVal);
end
if isKeyPressed(186) then
writeFloat(CamY, CamYVal + IncrementVal * SpeedUpVal);
end
if isKeyPressed(222) then
writeFloat(CamY, CamYVal - IncrementVal * SpeedUpVal);
end
if isKeyPressed(VK_H) then
writeFloat(LookX, LookXVal + IncrementVal * SpeedUpVal);
end
if isKeyPressed(VK_J) then
writeFloat(LookX, LookXVal - IncrementVal * SpeedUpVal);
end
if isKeyPressed(VK_K) then
writeFloat(LookY, LookYVal + IncrementVal * SpeedUpVal);
end
if isKeyPressed(VK_L) then
writeFloat(LookY, LookYVal - IncrementVal * SpeedUpVal);
end
if isKeyPressed(VK_U) then
writeFloat(LookZ, LookZVal + IncrementVal * SpeedUpVal);
end
if isKeyPressed(VK_I) then
writeFloat(LookZ, LookZVal - IncrementVal * SpeedUpVal);
end
if isKeyPressed(220) then
 if SlasHeld == 0 then
  SlasHeld = 1
  writeFloat(CamX, CamXVal * -1);
  writeFloat(CamZ, CamZVal * -1);
 else

 end
end
if not isKeyPressed(220) then
 SlasHeld = 0
end
end
