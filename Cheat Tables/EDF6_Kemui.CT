timer_setInterval( t, 10 )
local ActWind = getOpenedProcessID()
local IncrementVal = 1
local SpeedUpVal = 1
local SlasHeld = 0
local InitialDelay = 0

timer_onTimer( t, function( )
if InitialDelay == 10 then
 f.show()
 InitialDelay = 23
else
 InitialDelay = InitialDelay + 1
end

